                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>ibm.mq.commonservices</id>
                        <phase>validate</phase>
                        <configuration>
