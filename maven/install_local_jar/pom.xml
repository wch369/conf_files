                        <configuration>
                            <file>${project.basedir}/lib/com.ibm.mq.pcf-7.5.0.2.jar</file>
                            <repositoryLayout>default</repositoryLayout>
                            <groupId>ibm</groupId>
                            <artifactId>com.ibm.mq.pcf</artifactId>
                            <version>7.5.0.2</version>
                            <packaging>jar</packaging>
                            <generatePom>true</generatePom>
                        </configuration>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>ibm.mq.headers</id>
                        <phase>validate</phase>
                        <configuration>
                            <file>${project.basedir}/lib/com.ibm.mq.headers-7.5.0.2.jar</file>
                            <repositoryLayout>default</repositoryLayout>
                            <groupId>ibm</groupId>
                            <artifactId>com.ibm.mq.headers</artifactId>
                            <version>7.5.0.2</version>
